{
	"name": "@draftlab/auth-router",
	"version": "0.2.0",
	"description": "Minimal http router implementation for @draftlab/auth",
	"author": "Matheus Pergoli",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Draft-Lab/auth.git",
		"directory": "packages/router"
	},
	"homepage": "https://github.com/Draft-Lab/auth",
	"keywords": [
		"router",
		"http",
		"auth",
		"draftlab",
		"typescript"
	],
	"scripts": {
		"clean": "rm -rf dist .turbo node_modules",
		"build": "tsdown",
		"typecheck": "tsc --noEmit --emitDeclarationOnly false"
	},
	"type": "module",
	"main": "dist/index.mjs",
	"typings": "dist/index.d.mts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.mts"
		},
		"./*": {
			"import": "./dist/*.mjs",
			"types": "./dist/*.d.mts"
		}
	},
	"sideEffects": false,
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18"
	},
	"devDependencies": {
		"@draftlab/tsconfig": "workspace:*",
		"tsdown": "^0.16.7",
		"typescript": "^5.9.3"
	},
	"publishConfig": {
		"access": "public"
	}
}
